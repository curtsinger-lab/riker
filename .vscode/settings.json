    "**/.rkr/**": true,
    "benchmark.local/**": true,
    "**/__pycache__": true,
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/.rkr/**": true,
    "benchmark.local/**": true,
    "tests/**": true,
    "**/benchmarks/*/rkr-commands/**": true,
    "**/benchmarks/*/make-commands/**": true,
    "**/benchmarks/**/checkout/**": true,
  },
  "search.exclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/.rkr/**": true,
    "deps/**": true,
    "benchmarks/*/rkr-commands/**": true,
    "benchmarks/*/make-commands/**": true,
    "benchmark.local/**": true,
    "benchmarks/**/checkout/**": true,
  },
  "editor.rulers": [
    100
  ],
  "git.ignoreLimitWarning": true,
  "python.formatting.provider": "black",
